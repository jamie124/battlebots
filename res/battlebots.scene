scene 
{
    path = res/game.gpb
    
    node character 
    {
        collisionObject = res/battlebots.physics#character
    }
    
    node camera
    {
        collisionObject = res/battlebots.physics#camera
    }
    
    
    node characterMesh
    {
        
        material = res/battlebots.material#yellow
        tags
        {
            dynamic
        }
    }
    
    
    node testRoomFloor
    {
        collisionObject = res/battlebots.physics#staticBox
        
        material = res/battlebots.material#concreteFloor

    }
    
	node testRoomNorthWall
	{
		collisionObject = res/battlebots.physics#staticBox

		material = res/battlebots.material#wall

	}

	node testRoomSouthWall
	{
		collisionObject = res/battlebots.physics#staticBox

		material = res/battlebots.material#wall

	}

	node testRoomEastWall
	{
		collisionObject = res/battlebots.physics#staticBox

		material = res/battlebots.material#wall

	}

    physics
    {
        // Note: Gravity is set to 3x normal gravity to compenate for larger scale of scene.
        gravity = 0.0, -29.4, 0.0
    }
}